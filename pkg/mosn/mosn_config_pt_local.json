{
  "servers": [
    {
      "network_filters": [
        {
          "type": "proxy",
          "config": {
            "downstream_protocol": "SofaRpc",
            "upstream_protocol": "SofaRpc",
            "routes": [
              {
                "name": "example_route",
                "service": ".*",
                "cluster": "local_service"
              }
            ]
          }
        }
      ],
      "listeners": [
        {
          "name": "default_sofa",
          "address": "30.55.208.30:12200",
          "bind_port": true
        }
      ]
    }
  ],
  "tracing": {
    "http": {
      "driver": {
        "type": "zipkin",
        "config": {
          "collector_cluster": "jaeger",
          "collector_endpoint": "/api/v1/spans"
        }
      }
    }
  },
  "cluster_manager": {
    "clusters": [
      {
        "name": "local_service",
        "type": "SIMPLE",
        "lb_type": "LB_RANDOM",
        "max_request_per_conn": 100000,
        "healthcheck": {
          "timeout": 1000,
          "healthy_threshold": 3,
          "unhealthy_threshold": 5,
          "interval": 6000,
          "interval_jitter": 1000
        },
        "hosts": [
          {
            "hostname": "local_upstream",
            "address": "11.166.54.161:12200",
            "weight": 1
          }
        ]
      }
    ]
  }
}